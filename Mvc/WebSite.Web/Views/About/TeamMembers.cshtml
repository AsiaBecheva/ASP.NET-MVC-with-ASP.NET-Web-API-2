@model List<WebSite.Models.TeamMember>

<section class="wow bounceInRight container-fluid" id="articles">
    <header class="row">
        <h1>Team Members</h1>
    </header>
    <article class="row" style="display:inline-block; overflow-x:scroll; height:300px;">
        @{
            foreach (var member in this.Model)
            {
                <article class="col-md-2">
                    <img src="~/@(member.Image.Location + member.Image.OriginalFileName)" style="width:100%; height:120px;" />
                    <h3 style="font-weight:800">@member.Name</h3>
                    <h3>@member.Position</h3>
                    <p>@member.Description</p>
                </article>
            };
        }
    </article>
</section>